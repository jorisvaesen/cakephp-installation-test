{
  "name": "jorisvaesen/cakephp-installation-test",
  "description": "Test if required files exist after installation or composer update",
  "type": "cakephp-plugin",
  "require": {
    "php": ">=7.0",
    "cakephp/cakephp": "^3.7",
    "composer/composer": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7|^6.0"
  },
  "autoload": {
    "psr-4": {
      "InstallationTest\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "InstallationTest\\Test\\": "tests",
      "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
    }
  }
}
